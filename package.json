    "deploy": "firebase deploy",
    "gcloud-build": "cross-env gcloud builds submit --config ./cloudbuild.yaml ./ --substitutions=_CONFIG=\"${CONFIG}\",_FIREBASE_TOKEN=\"${FIREBASETOKEN}\"",
