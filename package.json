  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/recommended",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "vue/html-indent": [
        "error",
        "tab"
      ],
      "semi": [
        "error",
        "always"
      ]
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
